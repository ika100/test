pipeline {
  agent any
  stages {
    stage('say hello') {
      steps {
        echo 'hello'
      }
    }
    stage('say hello 2') {
      steps {
        echo 'hello 2'
      }
    }
    stage('say good hello3') {
      steps {
        echo 'hello3'
      }
    }
    stage('say good hello4') {
      steps {
        echo 'hello4'
      }
    }
  }
}
